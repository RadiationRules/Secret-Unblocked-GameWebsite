<script>
  // FAQ toggle expand/collapse on dt focus/enter or click
  document.querySelectorAll('#faq dt').forEach(dt => {
    dt.addEventListener('click', () => {
      const dd = dt.nextElementSibling;
      if (!dd) return;
      dd.style.display = dd.style.display === 'block' ? 'none' : 'block';
    });
    dt.addEventListener('keydown', e => {
      if (e.key === 'Enter' || e.key === ' ') {
        e.preventDefault();
        dt.click();
      }
    });
    // Initially hide all dd
    const dd = dt.nextElementSibling;
    if (dd) dd.style.display = 'none';
  });

  // Contact form validation and simple message
  const contactForm = document.getElementById('contact-form');
  const contactMsg = document.getElementById('contact-form-msg');
  contactForm.addEventListener('submit', e => {
    e.preventDefault();
    const name = contactForm['contact-name'].value.trim();
    const email = contactForm['contact-email'].value.trim();
    const message = contactForm['contact-message'].value.trim();
    if (!name || !email || !message) {
      contactMsg.style.color = '#d22';
      contactMsg.textContent = 'Please fill out all fields.';
      return;
    }
    // Basic email regex
    const emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
    if (!emailRegex.test(email)) {
      contactMsg.style.color = '#d22';
      contactMsg.textContent = 'Please enter a valid email.';
      return;
    }
    contactMsg.style.color = '#0b3d91';
    contactMsg.textContent = 'Thank you for reaching out! We will get back to you soon.';
    contactForm.reset();
  });

  // Smooth scrolling for nav buttons (assumes nav buttons have data-target attributes)
  document.querySelectorAll('nav button.tab').forEach(button => {
    button.addEventListener('click', e => {
      e.preventDefault();
      const target = button.getAttribute('data-target');
      if (!target) return;
      // Show target section, hide others
      document.querySelectorAll('main section').forEach(sec => {
        sec.hidden = sec.id !== target;
      });
      // Update active tab styles
      document.querySelectorAll('nav button.tab').forEach(btn => {
        btn.classList.toggle('active', btn === button);
        btn.setAttribute('aria-selected', btn === button ? 'true' : 'false');
        btn.setAttribute('tabindex', btn === button ? '0' : '-1');
      });
      // Focus the new section
      const targetSection = document.getElementById(target);
      if (targetSection) targetSection.focus();
      // Scroll to top smoothly
      window.scrollTo({top: 0, behavior: 'smooth'});
    });
  });

  // Keyboard accessibility for nav (left/right arrows)
  const navTabs = Array.from(document.querySelectorAll('nav button.tab'));
  navTabs.forEach((tab, index) => {
    tab.addEventListener('keydown', e => {
      if (e.key === 'ArrowRight') {
        e.preventDefault();
        const nextIndex = (index + 1) % navTabs.length;
        navTabs[nextIndex].focus();
      } else if (e.key === 'ArrowLeft') {
        e.preventDefault();
        const prevIndex = (index - 1 + navTabs.length) % navTabs.length;
        navTabs[prevIndex].focus();
      }
    });
  });
</script>

