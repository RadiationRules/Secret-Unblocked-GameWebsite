<script>
  // Resources Tab functionality
  const resourceTabs = document.querySelectorAll('.resource-tab');
  const resourcePanels = document.querySelectorAll('.resource-content');

  resourceTabs.forEach((tab, idx) => {
    tab.addEventListener('click', () => {
      resourceTabs.forEach(t => {
        t.classList.remove('active');
        t.setAttribute('aria-selected', 'false');
        t.setAttribute('tabindex', '-1');
      });
      tab.classList.add('active');
      tab.setAttribute('aria-selected', 'true');
      tab.setAttribute('tabindex', '0');

      resourcePanels.forEach(panel => panel.hidden = true);
      resourcePanels[idx].hidden = false;
      resourcePanels[idx].focus();
    });

    tab.addEventListener('keydown', (e) => {
      if(e.key === 'ArrowRight') {
        e.preventDefault();
        const nextIdx = (idx + 1) % resourceTabs.length;
        resourceTabs[nextIdx].focus();
      } else if(e.key === 'ArrowLeft') {
        e.preventDefault();
        const prevIdx = (idx - 1 + resourceTabs.length) % resourceTabs.length;
        resourceTabs[prevIdx].focus();
      }
    });
  });
</script>
