<script>
  // Huge games list - mix of popular and lesser-known unblocked games
  const gamesData = [
    { name: "Shell Shockers", category: "shooter", url: "https://shellshock.io/", desc: "Egg-citing online FPS shooter with weapons." },
    { name: "Krunker.io", category: "shooter", url: "https://krunker.io/", desc: "Fast-paced pixel FPS browser game." },
    { name: "Agar.io", category: "strategy", url: "https://agar.io/", desc: "Grow your cell by eating others." },
    { name: "Diep.io", category: "shooter", url: "https://diep.io/", desc: "Tank battles and upgrades in multiplayer arena." },
    { name: "Slither.io", category: "strategy", url: "https://slither.io/", desc: "Snake game with multiplayer twist." },
    { name: "Surviv.io", category: "shooter", url: "https://surviv.io/", desc: "2D battle royale with many players." },
    { name: "Little Big Snake", category: "strategy", url: "https://littlebigsnake.com/", desc: "Evolve your snake in multiplayer world." },
    { name: "Zombs Royale", category: "shooter", url: "https://zombsroyale.io/", desc: "Battle Royale with 100 players." },
    { name: "Bonk.io", category: "action", url: "https://bonk.io/", desc: "Physics-based multiplayer game." },
    { name: "Cookie Clicker", category: "puzzle", url: "https://orteil.dashnet.org/cookieclicker/", desc: "Idle clicker game for fun." },
    { name: "Tank Trouble", category: "shooter", url: "https://tanktrouble.com/", desc: "Tank battle in maze." },
    { name: "Superhot", category: "shooter", url: "https://superhotgame.com/", desc: "Time moves only when you move." },
    { name: "Moomoo.io", category: "strategy", url: "https://moomoo.io/", desc: "Build and battle in real-time." },
    { name: "Paper.io", category: "strategy", url: "https://paper-io.com/", desc: "Claim territory in multiplayer arena." },
    { name: "Starblast.io", category: "shooter", url: "https://starblast.io/", desc: "Space shooter with upgrades." },
    { name: "Stick Fight", category: "action", url: "https://stickfightthegame.com/", desc: "Physics brawler with stick figures." },
    { name: "War Brokers", category: "shooter", url: "https://warbrokers.io/", desc: "FPS with vehicles and many weapons." },
    { name: "Duck Life", category: "puzzle", url: "https://ducklife.com/", desc: "Train ducks and race in mini-games." },
    { name: "Realm of the Mad God", category: "multiplayer", url: "https://www.realmofthemadgod.com/", desc: "Co-op bullet hell MMO shooter." },
    { name: "Slay.one", category: "shooter", url: "https://slay.one/", desc: "Fast 2D shooter MMO." },
    { name: "Zombs.io", category: "strategy", url: "https://zombs.io/", desc: "Defend base and gather resources." },
    { name: "Happy Wheels", category: "action", url: "https://www.totaljerkface.com/happy_wheels.tjf", desc: "Fun ragdoll physics game." },
    { name: "Tank Wars", category: "shooter", url: "https://tankwars.io/", desc: "Classic tank shooter game." },
    { name: "Run 3", category: "puzzle", url: "https://run3.io/", desc: "Endless runner in space tunnels." },
    { name: "Fireboy and Watergirl", category: "puzzle", url: "https://www.coolmathgames.com/0-fireboy-and-watergirl-in-the-forest-temple", desc: "Co-op puzzle platformer." },
    { name: "Getaway Shootout", category: "action", url: "https://getawayshootout.com/", desc: "Competitive platform shooter." },
    { name: "Bonk.io", category: "action", url: "https://bonk.io/", desc: "Physics multiplayer game." },
    { name: "Super Smash Flash 2", category: "action", url: "https://www.super-smash-flash.com/", desc: "Smash Bros style fighter." },
    { name: "Shell Shockers", category: "shooter", url: "https://shellshock.io/", desc: "FPS with eggs." },
    { name: "Cookie Clicker", category: "puzzle", url: "https://orteil.dashnet.org/cookieclicker/", desc: "Idle game." },
    { name: "Fortnite", category: "shooter", url: "https://www.epicgames.com/fortnite/en-US/home", desc: "Popular battle royale game." },
    { name: "Minecraft Classic", category: "strategy", url: "https://classic.minecraft.net/", desc: "Classic block building game." },
    { name: "Among Us", category: "multiplayer", url: "https://innersloth.com/gameAmongUs.php", desc: "Social deduction multiplayer." },
    { name: "ROBLOX", category: "multiplayer", url: "https://www.roblox.com/", desc: "User-created games platform." },
    { name: "Krunker.io", category: "shooter", url: "https://krunker.io/", desc: "Pixel FPS game." },
    { name: "Google Feud", category: "puzzle", url: "https://www.googlefeud.com/", desc: "Guess the top Google searches." },
    { name: "2048", category: "puzzle", url: "https://play2048.co/", desc: "Classic number puzzle game." }
  ];

  const gamesList = document.querySelector('.games-list');
  const searchInput = document.getElementById('game-search');
  const categoryButtons = document.querySelectorAll('.category-btn');
  const resultsCount = document.querySelector('.search-results-count');

  function renderGames(filterCategory = 'all', searchTerm = '') {
    const filteredGames = gamesData.filter(game => {
      const matchesCategory = filterCategory === 'all' || game.category === filterCategory;
      const matchesSearch = game.name.toLowerCase().includes(searchTerm.toLowerCase());
      return matchesCategory && matchesSearch;
    });

    gamesList.innerHTML = '';
    if(filteredGames.length === 0) {
      gamesList.innerHTML = `<li tabindex="0" class="no-results">No games found.</li>`;
      resultsCount.textContent = "0 results";
      return;
    }

    filteredGames.forEach(game => {
      const li = document.createElement('li');
      li.tabIndex = 0;
      const link = document.createElement('a');
      link.href = game.url;
      link.textContent = game.name;
      link.className = 'game-link';
      link.target = '_blank';
      link.rel = 'noopener noreferrer';
      link.setAttribute('aria-label', `Play ${game.name} - ${game.desc}`);

      const desc = document.createElement('p');
      desc.textContent = game.desc;
      desc.className = 'game-desc';

      li.appendChild(link);
      li.appendChild(desc);
      gamesList.appendChild(li);
    });
    resultsCount.textContent = `${filteredGames.length} result${filteredGames.length > 1 ? 's' : ''}`;
  }

  // Initial render
  renderGames();

  // Category buttons logic
  categoryButtons.forEach(btn => {
    btn.addEventListener('click', () => {
      categoryButtons.forEach(b => {
        b.setAttribute('aria-pressed', 'false');
        b.classList.remove('active');
      });
      btn.setAttribute('aria-pressed', 'true');
      btn.classList.add('active');

      const cat = btn.getAttribute('data-category');
      const term = searchInput.value.trim();
      renderGames(cat, term);
    });
  });

  // Search input logic with debounce
  let debounceTimer;
  searchInput.addEventListener('input', () => {
    clearTimeout(debounceTimer);
    debounceTimer = setTimeout(() => {
      const term = searchInput.value.trim();
      const activeBtn = document.querySelector('.category-btn.active');
      const cat = activeBtn ? activeBtn.getAttribute('data-category') : 'all';
      renderGames(cat, term);
    }, 300);
  });

  // Keyboard accessibility: allow keyboard navigation on categories and games list
  categoryButtons.forEach((btn, i) => {
    btn.addEventListener('keydown', e => {
      if(e.key === 'ArrowRight' || e.key === 'ArrowDown') {
        e.preventDefault();
        const nextIndex = (i + 1) % categoryButtons.length;
        categoryButtons[nextIndex].focus();
      }
      if(e.key === 'ArrowLeft' || e.key === 'ArrowUp') {
        e.preventDefault();
        const prevIndex = (i - 1 + categoryButtons.length) % categoryButtons.length;
        categoryButtons[prevIndex].focus();
      }
    });
  });

  gamesList.addEventListener('keydown', e => {
    if(e.key === 'ArrowDown') {
      e.preventDefault();
      if(e.target.nextElementSibling) e.target.nextElementSibling.focus();
    }
    if(e.key === 'ArrowUp') {
      e.preventDefault();
      if(e.target.previousElementSibling) e.target.previousElementSibling.focus();
    }
  });
</script>
