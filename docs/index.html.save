  // Render games list
  function renderGames(filter = "") {
    const container = document.getElementById("games-list");
    container.innerHTML = "";
    const filtered = gamesData.filter(g =>
      g.name.toLowerCase().includes(filter.toLowerCase())
    );
    if (filtered.length === 0) {
      container.innerHTML = "<p>No games found matching your search.</p>";
      return;
    }
    filtered.forEach(game => {
      const el = document.createElement("a");
      el.href = game.url;
      el.target = "_blank";
      el.rel = "noopener noreferrer";
      el.className = "game-item";
      el.textContent = game.name;
      container.appendChild(el);
    });
  }
  renderGames();

  // Search logic
  document.getElementById("game-search").addEventListener("input", e => {
    renderGames(e.target.value);
  });

  // Admin login logic
  const adminForm = document.getElementById("admin-form");
  const adminCodeInput = document.getElementById("admin-code");
  const adminMessage = document.getElementById("admin-message");
  const adminSection = document.getElementById("admin");
  const hackerSection = document.getElementById("admin-hacker");
  const tabs = document.querySelectorAll("nav button.tab");
  const sections = document.querySelectorAll("main section");

  adminForm.addEventListener("submit", (e) => {
    e.preventDefault();
    const enteredCode = adminCodeInput.value.trim().toLowerCase();
    const correctCode = "radiationrules";
    if (enteredCode === correctCode) {
      adminMessage.style.color = "#0f0";
      adminMessage.textContent = "Access granted!";
      // Show hacker section and hide all else
      sections.forEach(s => (s.hidden = true));
      hackerSection.hidden = false;
      hackerSection.focus();
      tabs.forEach(t => t.classList.remove("active"));
    } else {
      adminMessage.style.color = "#d22";
      adminMessage.textContent = "Incorrect admin code. Try again.";
      adminCodeInput.value = "";
      adminCodeInput.focus();
    }
  });

  // Cancel button returns to Home tab
  document.getElementById("admin-cancel").addEventListener("click", () => {
    adminMessage.textContent = "";
    adminCodeInput.value = "";
    adminSection.hidden = true;
    // Show home section
    sections.forEach(s => {
      s.hidden = s.id !== "home";
      s.classList.toggle("active", s.id === "home");
    });
    tabs.forEach(t => {
      t.classList.toggle("active", t.getAttribute("data-target") === "home");
      t.setAttribute("aria-selected", t.getAttribute("data-target") === "home");
      t.setAttribute("tabindex", t.getAttribute("data-target") === "home" ? "0" : "-1");
    });
    document.getElementById("home").focus();
  });

  // MATRIX BACKGROUND ANIMATION FOR HACKER THEME
  (() => {
    const canvas = document.getElementById('matrix-canvas');
    const ctx = canvas.getContext('2d');
    let width, height;
    let columns;
    let drops;

    function resize() {
      width = canvas.width = canvas.offsetWidth;
      height = canvas.height = canvas.offsetHeight;
      columns = Math.floor(width / 14);
      drops = new Array(columns).fill(1);
    }

    function draw() {
      ctx.fillStyle = 'rgba(0, 0, 0, 0.05)';
      ctx.fillRect(0, 0, width, height);
      ctx.fillStyle = '#0F0';
      ctx.font = '14px monospace';
      for(let i = 0; i < drops.length; i++) {
        const text = String.fromCharCode(0x30A0 + Math.random() * 96);
        ctx.fillText(text, i * 14, drops[i] * 14);
        if(drops[i] * 14 > height && Math.random() > 0.975) {
          drops[i] = 0;
        }
        drops[i]++;
      }
    }

    window.addEventListener('resize', resize);
    resize();
    setInterval(draw, 50);
  })();
</script>
