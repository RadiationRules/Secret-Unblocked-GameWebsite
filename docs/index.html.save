<script>
  // Calculator logic
  (() => {
    const display = document.getElementById('calc-display');
    let currentInput = '';

    function updateDisplay() {
      display.value = currentInput || '0';
    }

    function clearDisplay() {
      currentInput = '';
      updateDisplay();
    }

    function calculateResult() {
      try {
        // eslint-disable-next-line no-eval
        let result = eval(currentInput);
        if (typeof result === 'number') {
          result = Math.round((result + Number.EPSILON) * 1000000) / 1000000; // Round to 6 decimals
        }
        currentInput = result.toString();
        updateDisplay();
      } catch {
        currentInput = 'Error';
        updateDisplay();
      }
    }

    document.getElementById('calc-buttons').addEventListener('click', (e) => {
      if (e.target.tagName !== 'BUTTON') return;
      const val = e.target.getAttribute('data-calc');
      if (val !== null) {
        if (currentInput === 'Error') currentInput = '';
        currentInput += val;
        updateDisplay();
      }
    });

    document.getElementById('calc-clear').addEventListener('click', () => {
      clearDisplay();
    });

    document.getElementById('calc-equals').addEventListener('click', () => {
      calculateResult();
    });

    updateDisplay();
  })();

  // Timer logic
  (() => {
    let timerInterval = null;
    const display = document.getElementById('timer-display');
    const input = document.getElementById('timer-input');
    const startBtn = document.getElementById('timer-start');
    const resetBtn = document.getElementById('timer-reset');
    let remainingSeconds = Number(input.value);

    function updateTimerDisplay(seconds) {
      display.textContent = seconds;
    }

    function startTimer() {
      if (timerInterval) clearInterval(timerInterval);
      remainingSeconds = Number(input.value);
      if (remainingSeconds <= 0 || isNaN(remainingSeconds)) return;
      updateTimerDisplay(remainingSeconds);
      timerInterval = setInterval(() => {
        remainingSeconds--;
        if (remainingSeconds <= 0) {
          clearInterval(timerInterval);
          display.textContent = "Time's up!";
          return;
        }
        updateTimerDisplay(remainingSeconds);
      }, 1000);
    }

    function resetTimer() {
      if (timerInterval) clearInterval(timerInterval);
      remainingSeconds = Number(input.value);
      updateTimerDisplay(remainingSeconds);
    }

    startBtn.addEventListener('click', startTimer);
    resetBtn.addEventListener('click', resetTimer);
    resetTimer();
  })();

  // Length converter logic (meters to feet)
  (() => {
    const inputMeters = document.getElementById('input-meters');
    const outputFeet = document.getElementById('output-feet');

    function convertMetersToFeet(m) {
      return (m * 3.28084).toFixed(2);
    }

    function updateConversion() {
      const val = Number(inputMeters.value);
      if (val < 0 || isNaN(val)) {
        outputFeet.textContent = 'Invalid input';
        return;
      }
      outputFeet.textContent = convertMetersToFeet(val);
    }

    inputMeters.addEventListener('input', updateConversion);
    updateConversion();
  })();
</script>
