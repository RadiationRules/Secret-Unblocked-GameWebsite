<script>
  const adminSection = document.getElementById('admin-access');
  const form = document.getElementById('admin-code-form');
  const codeInput = document.getElementById('admin-code-input');
  const errorMessage = document.getElementById('admin-code-error');
  const adminPanel = document.getElementById('admin-panel');
  const cancelBtn = document.getElementById('cancel-admin-access');
  const logoutBtn = document.getElementById('admin-logout');
  const secretGamesList = document.getElementById('secret-games-list');
  const hackerBg = document.getElementById('hacker-bg');
  const codeStream = document.getElementById('code-stream');

  // Secret admin code (case insensitive)
  const ADMIN_CODE = "RadiationRules".toLowerCase();

  // Secret games for admin panel
  const secretGames = [
    { name: "Shell Shockers", url: "https://shellshock.io/" },
    { name: "Krunker.io", url: "https://krunker.io/" },
    { name: "Diep.io", url: "https://diep.io/" },
    { name: "Surviv.io", url: "https://surviv.io/" },
    { name: "Zombs Royale", url: "https://zombsroyale.io/" },
    { name: "Bonk.io", url: "https://bonk.io/" },
    { name: "Realm of the Mad God", url: "https://www.realmofthemadgod.com/" },
    { name: "Slay.one", url: "https://slay.one/" },
    { name: "War Brokers", url: "https://warbrokers.io/" },
    { name: "Run 3", url: "https://run3.io/" },
    { name: "Among Us", url: "https://innersloth.com/gameAmongUs.php" },
    { name: "ROBLOX", url: "https://www.roblox.com/" }
  ];

  // Generate a long stream of random "code" for hacker background effect
  function generateCodeStream(lines = 1000) {
    const chars = "0123456789ABCDEFabcdef<>\\/{}[]();=+-*#%$@!";
    let stream = "";
    for (let i = 0; i < lines; i++) {
      let line = "";
      let length = Math.floor(Math.random() * 80) + 20;
      for (let j = 0; j < length; j++) {
        line += chars.charAt(Math.floor(Math.random() * chars.length));
      }
      stream += line + "\n";
    }
    return stream;
  }

  // Initialize code stream
  codeStream.textContent = generateCodeStream();

  // Show admin panel and games list after successful code entry
  function showAdminPanel() {
    errorMessage.textContent = "";
    adminPanel.hidden = false;
    codeInput.disabled = true;
    form.querySelector('button[type="submit"]').disabled = true;
    cancelBtn.disabled = true;
    hackerBg.style.display = "block";
    adminPanel.focus();

    // Populate secret games list
    secretGamesList.innerHTML = "";
    secretGames.forEach(game => {
      const li = document.createElement('li');
      const a = document.createElement('a');
      a.href = game.url;
      a.textContent = game.name;
      a.target = "_blank";
      a.rel = "noopener noreferrer";
      a.setAttribute('aria-label', `Play secret game ${game.name}`);
      li.appendChild(a);
      secretGamesList.appendChild(li);
    });
  }

  // Hide admin panel and reset form
  function hideAdminPanel() {
    adminPanel.hidden = true;
    hackerBg.style.display = "none";
    codeInput.disabled = false;
    codeInput.value = "";
    form.querySelector('button[type="submit"]').disabled = false;
    cancelBtn.disabled = false;
    errorMessage.textContent = "";
    codeInput.focus();
  }

  form.addEventListener('submit', (e) => {
    e.preventDefault();
    const inputVal = codeInput.value.trim().toLowerCase();
    if(inputVal === ADMIN_CODE) {
      showAdminPanel();
    } else {
      errorMessage.textContent = "Incorrect admin code. Please try again.";
      codeInput.focus();
    }
  });

  cancelBtn.addEventListener('click', () => {
    hideAdminPanel();
    // Return to home page
    adminSection.hidden = true;
    document.getElementById('homepage').hidden = false;
    document.getElementById('homepage').focus();
  });

  logoutBtn.addEventListener('click', () => {
    hideAdminPanel();
    adminSection.hidden = true;
    document.getElementById('homepage').hidden = false;
    document.getElementById('homepage').focus();
  });

  // Keyboard accessibility improvements
  codeInput.addEventListener('keydown', (e) => {
    if(e.key === 'Enter') {
      form.requestSubmit();
    }
    if(e.key === 'Escape') {
      cancelBtn.click();
    }
  });
</script>
